# Tests control flow statements.

entrypoint {
  var a = 145;

  if a > 200 { a /= 7; }

  if a < 15 { a *= 4; }
  else { a *= 2; }

  if a < 10 { a -= 6; }
  else if a < 20 { a -= 8; }
  else if a < 50 { a -= 15; }
  else if a < 1000 { a -= 20; }
  else { a -= 50; }

  while a < 2000 { a += 45; }

  while a < 2010; a++ {}

  while a { break; }

  while a >= 70; a -= 56 { continue; }
}
