# Tests packing colors and packed color constants.

entrypoint {
  var color;
  mlog::pack_color(color, 0.78, 0.13, 0.25);  # packcolor color 0.78 0.13 0.25

  color = 0pffaf00;                           # set color %ffaf00ff
  color = 0pffaf00ff;                         # set color %ffaf00ff
  color = 0p_ff_af_00;                        # set color %ffaf00ff
  color = 0p_ff_af_00_ff;                     # set color %ffaf00ff
  mlog::draw_col(color);
}
