# Tests printing and flushing instructions.

const new_line = "\n";
const age = 23 - 11 / 365;
link message1 as message;

entrypoint {
  var text = "My age is: ";
  mlog::print(text);                    # print text
  mlog::print(age);                     # print  age
  mlog::print(new_line);                # print "\n"

  mlog::printflush(message);            # printflush message1
}
